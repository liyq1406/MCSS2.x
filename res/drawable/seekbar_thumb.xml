    <!-- outer circle -->
    <item>
        <shape android:shape="oval" >
            <solid android:color="#FFFFFFFF" />
        </shape>
    </item>
 
    <!-- inner shadow of outer circle -->
    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape android:shape="oval">
            <solid android:color="#FFbdcad6" />
        </shape>
    </item>
    
    
 
