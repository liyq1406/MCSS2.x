<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
    	<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    		<corners android:bottomLeftRadius="0dp" android:bottomRightRadius="10dip" android:topLeftRadius="0dp" android:topRightRadius="10dip" />
			<solid android:color="@color/bottom_candidate_select_pressed" />
		</shape>    
    </item>
    <item android:state_pressed="false" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
    		<corners android:bottomLeftRadius="0dp" android:bottomRightRadius="10dip" android:topLeftRadius="0dp" android:topRightRadius="10dip" />
			<solid android:color="@color/bottom_candidate_select_normal" />
		</shape>
    </item>
</selector>
